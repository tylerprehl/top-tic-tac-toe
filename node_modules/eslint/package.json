    "@wdio/concise-reporter": "^8.14.0",
    "@wdio/globals": "^8.14.6",
    "@wdio/mocha-framework": "^8.14.0",
    "babel-loader": "^8.0.5",
    "c8": "^7.12.0",
    "chai": "^4.0.1",
    "cheerio": "^0.22.0",
    "common-tags": "^1.8.0",
    "core-js": "^3.1.3",
    "ejs": "^3.0.2",
    "eslint": "file:.",
    "eslint-config-eslint": "file:packages/eslint-config-eslint",
