    "esprima": "^4.0.1",
    "fast-glob": "^3.2.11",
    "fs-teardown": "^0.1.3",
    "glob": "^7.1.6",
    "got": "^11.8.3",
    "gray-matter": "^4.0.3",
    "lint-staged": "^11.0.0",
    "load-perf": "^0.2.0",
    "markdownlint": "^0.25.1",
    "markdownlint-cli": "^0.31.1",
    "marked": "^4.0.8",
    "memfs": "^3.0.1",
    "mocha": "^8.3.2",
    "mocha-junit-reporter": "^2.0.0",
    "node-polyfill-webpack-plugin": "^1.0.3",
    "npm-license": "^0.3.3",
    "pirates": "^4.0.5",
    "progress": "^2.0.3",
    "proxyquire": "^2.0.1",
