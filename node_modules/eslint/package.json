    "shelljs": "^0.8.2",
    "sinon": "^11.0.0",
    "vite-plugin-commonjs": "^0.8.2",
    "webdriverio": "^8.14.6",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "yorkie": "^2.0.0"
  },
  "keywords": [
    "ast",
    "lint",
    "javascript",
    "ecmascript",
